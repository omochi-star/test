<% layout('layouts/boilerplate') %>

    <div class="container">
        <h3>
            読んだ本一覧
        </h3>
        <ul>
            <% for(let bookreview of formattedReviews) {%>
                <li>
                    <a href="bookreview/<%= bookreview._id  %>">
                        <%=bookreview.title %>
                    </a>
                    - <%= bookreview.createdAtFormatted %>
                </li>
                <% } %>
        </ul>
        <a href="/bookreview/new">新規登録へ</a>
    </div>